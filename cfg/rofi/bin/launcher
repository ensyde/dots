#!/usr/bin/env bash

DIR="/home/psy/.config/rofi/themes" 
rofi \
  -show drun \
  -modi drun,run \
  -no-lazy-grab \
  -scroll-method 0 \
  -drun-match-fields all \
  -drun-display-format "{name}" \
  -no-drun-show-actions \
  -terminal alacritty \
  -kb-cancel Alt-F1 \
  -theme "$DIR"/launcher.rasi 
